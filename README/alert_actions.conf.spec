[globalBanner]

param.visible = <boolean>
* Default: true

param.message = <string>
* Default: none

param.background_color = [green|blue|yellow|orange|red]
* Default: blue

param.hyperlink = [http://<string>|https://<string>]
* Default: none

param.hyperlink_text = <string>
* Default: none

param.trigger_time = <string>
* Field value of the trigger time of the alert
* Default: none

param.expiration_timeout = <time-specifier>
* Field value of the expiration timeout time of the alert
* Default: 24h

param.sid = <string>
* Field value of the search ID of the alert
* Default: none
